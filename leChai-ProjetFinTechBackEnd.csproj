    <ImplicitUsings>enable</ImplicitUsings>
    <RootNamespace>leChai_ProjetFinTechBackEnd</RootNamespace>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Microsoft.AspNetCore.OpenApi" Version="7.0.5" />
    <PackageReference Include="Microsoft.Extensions.Configuration" Version="7.0.0" />
    <PackageReference Include="Microsoft.Extensions.Configuration.Json" Version="7.0.0" />
    <PackageReference Include="Swashbuckle.AspNetCore" Version="6.4.0" />
    <PackageReference Include="System.Data.SqlClient" Version="4.8.5" />
    <Reference Include="ParserTools">
      <HintPath>InternalDependencies\ParserTools.dll</HintPath>
    </Reference>
    <Reference Include="QueryBuilder">
      <HintPath>InternalDependencies\QueryBuilder.dll</HintPath>
    </Reference>
    <Reference Include="SQLExecutorTools">
      <HintPath>InternalDependencies\SQLExecutorTools.dll</HintPath>
    </Reference>
  </ItemGroup>
